        //处理状态
        function ReturnStr(obj, value) {
            var html = "";
            if (obj == 0) {
                // bg-primary
                html = "<span class='badge LStyle' style='background-color:#337ab7;font-size:13px;'>" + value + "</span>";
            } else if (obj == 1) {
                // bg-orange-active
                html = "<span class='badge LStyle' style='background-color:#ff7701;font-size:13px;'>" + value + "</span>";
            } else if (obj == 2) {
                html = "<span class='badge bg-yellow LStyle' style='font-size:13px;'>" + value + "</span>";
            } else if (obj == 3) {
                html = "<span class='badge bg-green LStyle' style='font-size:13px;'>" + value + "</span>";
            } else if (obj == 4) {
                // label-sm bg-red
                html = "<span class='badge LStyle' style='background-color:#dd4b39;font-size:13px;'>" + value + "</span>";
            }
            return html;
