@{
    ViewBag.Title = "首页";
    ViewBag.Description = "产线管理信息系统";
}




<!-- 小方格(统计方格) -->
<div class="row">
    <div class="col-lg-3 col-xs-6">
        <div class="small-box bg-info">
            <div class="inner">
                <h3>用户数量</h3>
                <h4>@ViewBag.UsersCount 位</h4>
                @*<sup style="font-size: 20px">%</sup>*@
            </div>
            <div class="icon">
                <i class="icon ion-person-stalker"></i>
            </div>
            <a href="/System/Users" class="small-box-footer">详细信息 <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <div class="col-lg-3 col-xs-6">
        <div class="small-box bg-info">
            <!--olive-->
            <div class="inner">
                <h3>完成数量</h3>
                <h4>@ViewBag.CompleteCount 条</h4>
            </div>
            <div class="icon">
                @*<i class="fas fa-clipboard-check"></i>*@
                <i class="icon ion-android-checkmark-circle"></i>
            </div>
            <a href="/ProductInfo/AssemblyProd" class="small-box-footer">详细信息 <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <div class="col-lg-3 col-xs-6">
        <div class="small-box bg-blue">
            <div class="inner">
                <h3>合格数量</h3>
                <h4>@ViewBag.QualifiedCount 条</h4>
            </div>
            <div class="icon">
                <i class="icon ion-android-done-all"></i>
            </div>
            <a href="/ProductInfo/AssemblyProd" class="small-box-footer">详细信息 <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <div class="col-lg-3 col-xs-6">
        <div class="small-box bg-blue">
            <div class="inner">
                <h3>工位数量</h3>
                <h4>@ViewBag.DeviceCount 台</h4>
            </div>
            <div class="icon">
                <i class="icon ion-monitor"></i>
            </div>
            <a href="/BaseInfo/BaseGongWei" class="small-box-footer">详细信息 <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
</div>






